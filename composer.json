{
	"name": "slevomat/eet-client",
	"description": "Client for electronic records of sales (EET) in Czech republic",
	"license": "MIT",
	"type": "library",
	"keywords": ["eet", "electronic records of sales"],
	"authors": [
		{
			"name": "Slevomat.cz, s.r.o.",
			"homepage": "http://www.slevomat.cz"
		}
	],
	"config": {
		"sort-packages": true
	},
	"support": {
		"issues": "https://github.com/slevomat/eet-client/issues"
	},
	"require": {
		"php": "^7.2 || ^8.0",
		"ext-dom": "*",
		"ext-openssl": "*",
		"ext-soap": "*",
		"ramsey/uuid": "^3.5 || ^4.0",
		"robrichards/wse-php": "^2.0",
		"slevomat/consistence": "^2.1.0"
	},
	"require-dev": {
		"composer/ca-bundle": "^1.0",
		"guzzlehttp/guzzle": "^6.3.3 || ^7.0.1",
		"phing/phing": "^2.16.3",
		"php-parallel-lint/php-parallel-lint": "^1.2",
		"phpstan/phpstan": "^1.1.2",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpunit/phpunit": "^8.5.14 || ^9.3.7",
		"slevomat/coding-standard": "^6.3.10",
		"squizlabs/php_codesniffer": "^3.5.6"
	},
	"autoload": {
		"psr-4": {
			"SlevomatEET\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SlevomatEET\\": "tests/SlevomatEET"
		}
	},
	"suggest": {
		"guzzlehttp/guzzle": "For usage of bundled GuzzleSoapClientDriver"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		}
	}
}
